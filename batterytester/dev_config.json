{
  "platform": "windows",
  "test_logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "simple": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "formatter": "simple",
        "stream": "ext://sys.stdout"
      },
      "info_file_handler": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "INFO",
        "formatter": "simple",
        "filename": "logs/test_info.log",
        "maxBytes": 100024,
        "backupCount": 50,
        "encoding": "utf8"
      }
    },
    "root": {
      "level": "DEBUG",
      "handlers": [
        "console",
        "info_file_handler"
      ]
    }
  },
  "server_logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "simple": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "formatter": "simple",
        "stream": "ext://sys.stdout"
      }
    },
    "root": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    }
  },
  "test_configs": "../configs"
}